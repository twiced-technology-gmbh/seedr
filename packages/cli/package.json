{
  "name": "@toolr/seedr",
  "version": "0.1.59",
  "description": "Seed your projects with AI configurations",
  "type": "module",
  "bin": {
    "seedr": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/cli.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.1",
    "chalk": "^5.3.0",
    "commander": "^14.0.0",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "inquirer": "^13.0.0",
    "ora": "^9.0.0",
    "simple-git": "^3.32.3"
  },
  "devDependencies": {
    "@seedr/shared": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^25.0.0",
    "@vitest/coverage-v8": "^4.0.0",
    "memfs": "^4.14.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.0"
  },
  "keywords": [
    "ai",
    "claude",
    "copilot",
    "gemini",
    "codex",
    "opencode",
    "configuration",
    "seeder"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/twiced-technology-gmbh/seedr.git"
  },
  "homepage": "https://seedr.toolr.dev",
  "bugs": {
    "url": "https://github.com/twiced-technology-gmbh/seedr/issues"
  },
  "license": "MIT"
}
