{
  "name": "seedr-monorepo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "AI tools configuration seeder - part of the toolr-suite ecosystem",
  "scripts": {
    "prebuild": "tsx scripts/sync.ts",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "compile": "tsx scripts/compile-manifest.ts",
    "sync": "tsx scripts/sync.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-sonarjs": "^3.0.1",
    "eslint-plugin-unicorn": "^57.0.0",
    "tsx": "^4.19.0",
    "turbo": "^2.3.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.21.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20"
  }
}
